{
  "name": "angular2-basic",
  "version": "1.0.0-rc1",
  "license": "MIT",
  "scripts": {
    "build:pro": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run clean:dist && npm run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run clean:dist && npm run build:aot",
    "ci": "npm run lint && npm test && npm run e2e",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "protractor": "protractor config/protractor.conf.js",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "test": "npm run test:pro",
    "test:pro": "karma start",
    "test:dev": "karma start --dev",
    "tslint": "tslint",
    "docs:typedoc": "npm run typedoc -- --options config/typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs": "npm run compodoc",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -d docs/compodoc/ -s -o ",
    "typedoc": "typedoc",
    "clean:aot": "npm run rimraf -- compiled dist ",
    "clean:dll": "npm run rimraf -- dll",
    "clean:dist": "npm run rimraf -- dist compiled",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "rimraf": "rimraf",
    "serve:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/ --inline --hot",
    "serve:dev:dash": "webpack-dashboard -- webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/ --inline --hot",
    "serve:pro": "npm run build:pro && http-server dist --cors -o",
    "serve:aot": "npm run build:aot && http-server dist --cors -o",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "serve": "npm run serve:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
